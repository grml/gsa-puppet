SHELL=/bin/bash
12,34 * * * * root if [ -x /usr/sbin/puppetd ]; then sleep $(( $RANDOM \% 600 )) ; if [ -x /usr/bin/timeout ]; then TO="timeout 3600"; else TO=""; fi; $TO /usr/sbin/puppetd --report -o --no-daemonize; fi
