SHELL=/bin/bash
12,34 * * * * root if [ -x /usr/bin/puppet ]; then sleep $(( $RANDOM \% 600 ))
; if [ -x /usr/bin/timeout ]; then TO="timeout 3600"; else TO=""; fi; $TO
/usr/bin/puppet agent --onetime --no-daemonize; fi
